    "preact": "latest",
    "preact-compat": "latest",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-draggable": "^3.1.1",
    "react-final-form": "^4.0.2",
    "react-joystick-component": "^1.4.0",
    "react-keyboard-event-handler": "^1.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "latest",
    "react-sound": "^1.2.0",
    "react-virtualized": "^9.21.0",
